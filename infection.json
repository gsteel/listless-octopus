{
    "ignoreMsiWithNoMutations": true,
    "logs": {
        "text": "php://stderr",
        "github": true,
        "stryker": {
            "report": "/^\\d+\\.\\d+\\.x$/"
        }
    },
    "minCoveredMsi": 100,
    "minMsi": 100,
    "phpUnit": {
    },
    "source": {
        "directories": [
            "src"
        ]
    },
    "timeout": 10,
    "mutators": {
        "@default": true,
        "DecrementInteger": {
            "ignore": [
                "ListInterop\\Octopus\\Exception\\RequestFailure::withPsrError::24"
            ]
        },
        "IncrementInteger": {
            "ignore": [
                "ListInterop\\Octopus\\Exception\\RequestFailure::withPsrError::24"
            ]
        },
        "ArrayItemRemoval": {
            "ignore": [
                "ListInterop\\Octopus\\Value\\MailingList::fromArray::52",
                "ListInterop\\Octopus\\Value\\Contact::fromArray::44"
            ]
        }
    }
}
