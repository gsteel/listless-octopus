{
    "ignoreMsiWithNoMutations": true,
    "logs": {
        "text": "php://stderr",
        "github": true,
        "stryker": {
            "report": "/^\\d+\\.\\d+\\.x$/"
        }
    },
    "minCoveredMsi": 100,
    "minMsi": 100,
    "phpUnit": {
    },
    "source": {
        "directories": [
            "src"
        ]
    },
    "timeout": 10,
    "mutators": {
        "@default": true
    }
}
