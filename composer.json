{
    "name": "gsteel/listless-octopus",
    "type": "library",
    "description": "Listless implementation for Email Octopus",
    "keywords": [],
    "homepage": "https://github.com/gsteel/listless-octopus",
    "license": "MIT",
    "authors": [
        {
            "name": "George Steel",
            "email": "george@net-glue.co.uk"
        }
    ],
    "provide": {
        "symfony/polyfill-php73": "*"
    },
    "require": {
        "php": "^7.3 || ~8.0",
        "ext-json": "*",
        "gsteel/listless": "dev-main",
        "myclabs/php-enum": "^1.8",
        "php-http/discovery": "^1.11",
        "psr/container": "^1 || ^2",
        "psr/http-client": "^1.0",
        "psr/http-client-implementation": "*",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "doctrine/coding-standard": "^9.0",
        "ergebnis/composer-normalize": "^2.15",
        "infection/infection": "^0.18.2",
        "laminas/laminas-diactoros": "^2.6",
        "php-http/curl-client": "^2.2",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.16.1",
        "react/child-process": "^0.6.3",
        "react/http": "^1.4",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.8",
        "ext-curl": "*"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "composer-normalize": {
            "indent-size": 4,
            "indent-style": "space"
        }
    },
    "autoload": {
        "psr-4": {
            "GSteel\\Listless\\Octopus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GSteel\\Listless\\Octopus\\Test\\Unit\\": "test/Unit",
            "GSteel\\Listless\\Octopus\\Test\\Integration\\": "test/Integration"
        }
    },
    "scripts": {
        "check": [
            "phpcs",
            "psalm",
            "phpunit",
            "infection"
        ],
        "clean": [
            "composer normalize",
            "psalm --generate-baseline",
            "phpcbf"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gsteel/listless"
        }
    ]
}
